{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": false,
    "lib": [
      "es2019"
    ],
    "target": "es2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "sourceMap": true
  },
  "include": [
    "./src/**/*"
  ]
}
